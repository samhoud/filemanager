{
  "name": "samhoud/filemanager",
  "description": "Simple filesystem wrapper for Laravel 5.1",
  "homepage": "https://github.com/samhoud/filemanager",
  "keywords": ["filesystem", "flysystem", "image", "laravel"],
  "license": "MIT",
  "authors": [
    {
      "name": "Bas van Vliet",
      "email": "b.vanvliet@samhoud.com",
      "homepage": "http://samhoud.com"
    }
  ],
  "require": {
    "php": ">=5.5.9",
    "illuminate/support": "5.1.*",
    "illuminate/contracts": "5.1.*",
    "illuminate/filesystem": "5.1.*",
    "league/flysystem": "~1.0",
    "intervention/image": "2.2.*"
  },
  "require-dev": {
    "phpunit/phpunit": "4.7.*",
    "mockery/mockery": "~0.9.2",
    "symfony/var-dumper" : "2.7.*"
  },
  "autoload": {
    "psr-4": {
      "Samhoud\\FileManager\\": "src/Samhoud/FileManager"
    }
  },
  "minimum-stability": "stable"
}
